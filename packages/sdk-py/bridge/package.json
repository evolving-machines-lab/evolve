{
  "name": "evolve-sdk-python-bridge",
  "version": "0.0.4",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc && node bundle.mjs",
    "build:standalone": "npm run build:deps && npm run build",
