    "dev": "tsc --watch"
  },
  "dependencies": {
    "@evolvingmachines/e2b": "file:../../e2b",
    "@evolvingmachines/modal": "file:../../modal",
    "@evolvingmachines/sdk": "file:../../sdk-ts"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "esbuild": "^0.24.0",
    "typescript": "^5.8.3"
  }
}
