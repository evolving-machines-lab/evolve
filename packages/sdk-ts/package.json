    "test:codex": "tsx tests/integration/01-all-agents-parallel.ts codex",
    "test:gemini": "tsx tests/integration/01-all-agents-parallel.ts gemini",
    "test:qwen": "tsx tests/integration/01-all-agents-parallel.ts qwen"
